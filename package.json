{
  "name": "dao-api",
  "version": "1.0.0",
  "description": "GraphQL + MongoDB server for DAO",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"**/*.js\"",
    "format:ci": "prettier --list-different \"**/*.js\"",
    "seed": "babel-node server/seed.js",
    "server": "nodemon --exec babel-node server/index.js",
    "start": "npm run server",
    "start-seed": "npm run server & npm run seed",
    "test": "jest --run-in-band",
    "test:ci": "jest --run-in-band"
  },
  "contributors": [
    "Alan Lee <alanjlee89@gmail.com>",
    "Jacob Merrell <jacobscarfmerrell@gmail.com"
  ],
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@shelf/jest-mongodb": "^1.2.3",
    "apollo-server-testing": "^2.19.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "prettier": "^2.1.2",
    "regenerator-runtime": "^0.13.7"
  },
  "dependencies": {
    "apollo-server-express": "^2.19.0",
    "babel-jest": "^26.6.3",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^5.1.0",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.2.3",
    "graphql-yoga": "^1.18.3",
    "merge-graphql-schemas": "^1.7.8",
    "mongoose": "^5.10.7",
    "nodemon": "^2.0.4",
    "uuid": "^8.3.0"
  }
}
